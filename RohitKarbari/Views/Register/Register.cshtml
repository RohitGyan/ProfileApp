
@{
    ViewBag.Title = "Register";
}

<h2>Register</h2>
<div>
<form action="Register" method="post">
<input type=text class="form-control" placeholder="Name" name="Name" asp-for="Name" />
<br />
<input type=text placeholder="LastName" class="form-control" name="LastName" asp-for="LastName" />
<br />
<input type=text placeholder="Email" class="form-control" name="Email" asp-for="Email" />
<br />
<input type=text placeholder="Number" class="form-control" name="Number" asp-for="Number" />
<br />
<input type="submit" class="btn-success" />
</form>
</div>




@{
    ViewBag.Title = "Register";
}

<h2 style="text-align:center">Register</h2>
<div>
    <form action="Register" method="post" class="form">
            <input type=text class="form-control" id="floatingInput" placeholder="Name"  name="Name" asp-for="Name" />
        <br />
        <input type=text class="form-control" placeholder="LastName" name="LastName" asp-for="LastName" />
        <br />
        <input type=text class="form-control" placeholder="Email" name="Email" asp-for="Email" />
        <br />
        <input type=text class="form-control" placeholder="Number" name="Number" asp-for="Number" />
        <br />
        <input type="submit" class="btn-success" />
    </form>
</div>


